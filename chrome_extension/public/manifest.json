{
  "name": "Fantasy Free Agent",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "tabs",
    "storage",
    "declarativeContent",
    "activeTab",
    "cookies",
    "http://lvh.me/"
  ],
  "commands": {
    "getPlayers": {
      "suggested_key": {
        "default": "Alt + X"
      },
      "description": "Send list of players."
    }
  },
  "background": {
    "scripts": [
      "chrome-extension-async.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/baseball16.png",
      "32": "images/baseball32.png",
      "48": "images/baseball48.png",
      "128": "images/baseball32.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/baseball16.png",
    "32": "images/baseball32.png",
    "48": "images/baseball48.png",
    "128": "images/baseball128.png"
  },
  "manifest_version": 2
}